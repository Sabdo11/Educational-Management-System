<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @student.name %>
</p>

<p>
  <strong>Student ID:</strong>
  <%= @student.identifier %>
</p>

<p>
  <strong>Section ID:</strong>
<ol>
  <% @student.sections.each do |section| %>
    <li> <%= section.year %> </li>
  <% end %>
</ol>
</p>

<%= link_to 'Edit', edit_student_path(@student) %> |
<%= link_to 'Back', students_path %>
